#include <stdio.h>
int display(int arr[],int size);

int main()
{
    int n,i;
    printf("Enter elements in array");
    scanf("%d",&n);
    int arr[n];
    
    for(i=0;i<n;i++)
    {
         scanf("%d",&arr[i]);
    }
    display(arr,n);
    return 0;
}

int display(int arr[],int size)
{
    int j;
    printf("elements in thr array are\n");
    for(j=0;j<size;j++)
    {
       printf("%d\n",arr[j]);
    }
}